        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Calculate constant latency to the target server" enabled="true">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">SampleResult.setIgnore(); // Ignores this sampler from reporting

/*
 * Pseudo-distance from server to calculate constant latency per location
 * This code assumes the earth is flat and lattitude and longitude lines are equidistant for simplicity
 * 
 * Default server location is somewhere in San Francisco,CA
 */

 var AVG_LAT_DISTANCE_KM = 111;
 var AVG_LON_DISTANCE_KM = 85;
 
 var deviceLat = Double.parseDouble(props.get(&quot;lat&quot;,&quot;54.16&quot;));
 var deviceLon = Double.parseDouble(props.get(&quot;lon&quot;, &quot;18.36&quot;));
 var serverLat = Double.parseDouble(props.get(&quot;serverLat&quot;, &quot;36.7783&quot;));
 var serverLon = Double.parseDouble(props.get(&quot;serverLon&quot;, &quot;119.4179&quot;));

 var latDiff = serverLat - deviceLat;
 var lonDiff = serverLon - deviceLon;
 
 var latDistance = latDiff * AVG_LAT_DISTANCE_KM;
 var lonDistance = lonDiff * AVG_LON_DISTANCE_KM;
 
 var pseudoDistance = Math.sqrt(Math.pow(latDistance, 2) + Math.pow(lonDistance, 2));

props.put(&quot;CONSTANT_LATENCY&quot;, String.valueOf(Math.round(pseudoDistance / 300)));




 

 </stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
        <hashTree/>
      </hashTree>
